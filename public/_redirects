# _redirects for Cloudflare Pages - Corrected based on logs

# Specific static pages
/about /about.html 200

# More specific dynamic routes first
/:slug/:chapter /reader.html?mangaTitle=:slug&chapterKey=:chapter 200

# Less specific dynamic routes second
/:slug /series.html?mangaTitle=:slug 200

# 301 Redirects for old HTML paths to clean URLs (for SEO)
# These are for when someone directly accesses the old .html paths
/series.html / 301
/reader.html / 301 # Redirect generic /reader.html to homepage or a general list
/series.html?mangaTitle=:slug /:slug 301
/reader.html?mangaTitle=:slug&chapterKey=:chapter /:slug/:chapter 301
